<section id='schedule'>
    <div class='container local'>
        <div class="row">
            <div class="col-lg-12 text-center">
                <h1>Schedule</h1>
                <hr class="star-primary">
            </div>
            {{#days}}
                <div class="col-md-4">
                        <h4>{{day}}</h4>
                        <div class='list-group'>
                            {{#sessions}}
                                <a class='list-group-item'>{{.}}</a>
                            {{/sessions}}
                        </div>
                </div>
            {{/days}}
        </div>
    </div>
</section>